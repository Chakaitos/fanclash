<!-- Contact Modal -->
<div class="modal fade" id="contact" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <form class="form-horizontal">
        <div class="modal-body">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">X</button>
          <h3>Contact Us</h3><hr>
          <div class="form-group">
            <label for="contact-name" class="col-md-2 control-label">Name:</label>
            <div class="col-md-10">
              <input type="text" class="form-control" id="contact-name" placeholder="Full Name"></input>
            </div>
          </div>

          <div class="form-group">
            <label for="contact-email" class="col-md-2 control-label">Email:</label>
            <div class="col-md-10">
              <input type="email" class="form-control" id="contact-email" placeholder="you@example.com"></input>
            </div>
          </div>

          <div class="form-group">
            <label for="contact-message" class="col-md-2 control-label">Message:</label>
            <div class="col-md-10">
              <textarea class="form-control" rows="6"></textarea>
            </div>
          </div>
          <div class="modal-buttons"><button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>

            <button type="button" class="btn dk-button" data-dismiss="modal">Send Message</button>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>
<!-- End Contact Modal -->